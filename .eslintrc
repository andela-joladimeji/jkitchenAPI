{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jasmine": true,
    "jquery": true,
    "mocha": true
  },
  "globals": {
    "window": true
  },
  "rules": {
    "quotes": 0,
    "new-cap": 0,
    "no-console": 1,
    "no-debugger": 1,
    "no-param-reassign": 0,
    "no-var": 1,
    "semi": [1, "always"],
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
  }
}